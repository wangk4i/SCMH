            <PsychogenyPastDangerousCode version='1.0' value='$!{}'>$!{}</PsychogenyPastDangerousCode>
            <PsychogenyPastDangerousCode version='1.0' value='$!{}'>$!{}</PsychogenyPastDangerousCode>
        </PsychogenyPastDangerousCodeList>
        <PsychogenyPastRiskCode version='1.0' value='$!{RiskNowNam}'>$!{RiskNow}</PsychogenyPastRiskCode>
        <DiseaseCode version='1.0' value='$!{DischargeDiagnosisNam}'>$!{DischargeDiagnosis}</DiseaseCode>
        <DiagnoseTime>$!{DiagnosisDate}</DiagnoseTime>
        <IfNoMedication version='1.0' value='$!{IfNoMedicationNam}'>$!{IfNoMedication}</IfNoMedication>
        <DrugList>
            <DrugInfo>
                <DrugID>$!{MedicationId}</DrugID>
                <Sort>$!{SortNo}</Sort>
                <LongActingDrugFlagCode version='1.0' value='$!{IfLongActNam}'>$!{IfLongAct}</LongActingDrugFlagCode>
                <DrugCode version='1.0' value='$!{DrugNam}'>$!{DrugCode}</DrugCode>
                <DrugSpecifications>$!{LDrugSpecifications}</DrugSpecifications>
                <DrugDoseCode>$!{DrugUnit}</DrugDoseCode>
                <LongActingDrugFrequency>$!{LDrugFrequency}</LongActingDrugFrequency>
                <LongActingDrugFrequencyUnitCode version='1.0' value='$!{UseDailyNam}'>$!{UseDailyCode}</LongActingDrugFrequencyUnitCode>
                <LongActingDrugDose>$!{LDrugDose}</LongActingDrugDose>
            </DrugInfo>
            <DrugInfo>
                <DrugID>$!{MedicationId}</DrugID>
                <Sort>$!{SortNo}</Sort>
                <LongActingDrugFlagCode version='1.0' value='$!{IfLongActNam}'>$!{IfLongAct}</LongActingDrugFlagCode>
