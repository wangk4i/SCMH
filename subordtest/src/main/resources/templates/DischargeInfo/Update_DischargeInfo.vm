                <DrugSpecifications>$!{SDrugSpecifications}</DrugSpecifications>
                <DrugDoseCode>$!{DrugUnit}</DrugDoseCode>
                <NonLongActingDrugDoseMorning>$!{SDrugDosem}</NonLongActingDrugDoseMorning>
                <NonLongActingDrugDoseNoon>$!{SDrugDosen}</NonLongActingDrugDoseNoon>
                <NonLongActingDrugDoseEvening>$!{SDrugDosee}</NonLongActingDrugDoseEvening>
            </DrugInfo>
        </GuideDrugList>
        <NextMeasuresCodeList>
            <NextMeasuresCode version='1.0' value='$!{}'>$!{}</NextMeasuresCode>
            <NextMeasuresCode version='1.0' value='$!{}'>$!{}</NextMeasuresCode>
            <NextMeasuresCodeOther>$!{RehabilitationMeasuresDesc}</NextMeasuresCodeOther>
        </NextMeasuresCodeList>
        <NextMeasuresOther>$!{OtherConsider}</NextMeasuresOther>
        <DoctorName>$!{FillDoctorName}</DoctorName>
        <DoctorPhone>$!{DoctorTel}</DoctorPhone>
        <FillDate>$!{FillDate}</FillDate>
    </TreatmentInfo>
    </Body>
</EDRDocument>
