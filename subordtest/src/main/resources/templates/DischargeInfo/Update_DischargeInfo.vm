        <GuideDrugList>
            <DrugInfo>
                <DrugID>$!{MedicationId}</DrugID>
                <Sort>$!{SortNo}</Sort>
                <LongActingDrugFlagCode version='1.0' value='$!{IfLongActNam}'>$!{IfLongAct}</LongActingDrugFlagCode>
                <DrugCode version='1.0' value='$!{DrugNam}'>$!{DrugCode}</DrugCode>
                <DrugSpecifications>$!{LDrugSpecifications}</DrugSpecifications>
                <DrugDoseCode>$!{DrugUnit}</DrugDoseCode>
