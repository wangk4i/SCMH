    <PersonInfo>
        <PersonID>$!{BasicInformationId}</PersonID>
        <PatientName>$!{PatientName}</PatientName>
        <IdCardTypeCode version='1.0' value='$!{IDTypeNam}'>$!{IDType}</IdCardTypeCode>
